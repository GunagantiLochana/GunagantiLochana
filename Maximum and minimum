#include<stdio.h>
void main()
{
int a,b,c;
{printf("Enter three numbers");
scanf("%d",&a,&b,&c);
if(a>=b&&a>=c)
printf("%d is maximum number",a);
else if(b>=a&&b>=c)
printf("%d is maximum number",b);
else
printf("%d is maximum number",c);
}
if(a<=b&&a<=c)
printf("%d is minimum number",a);
else if(b<=a&&b<=c)
printf("%d is minimum number",b);
else
printf("%d is minimum number",c);
}
