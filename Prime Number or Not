// To check whether given number is prime number or not

#include<stdio.h>
void main()
{
int i, num, cnt=0;
printf("Enter a number");
scanf("%d",&num);
i=1;
while(i<=num)
{
if(num%i==0)
cnt++;
i++;
}
{
if(cnt==2)
printf("The number is a Prime Number");
else
printf("The number is not a prime number");
}
}
